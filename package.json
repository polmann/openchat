{
  "name": "openchat",
  "version": "0.0.1",
  "description": "a socket.io tutorial",
  "main": "server/index.js",
  "engines": {
    "node": "8.3.0"
  },
  "scripts": {
    "fix": "standard --fix",
    "start": "node server/index.js",
    "build": "webpack",
    "dev:start": "nodemon server/index.js",
    "dev:build": "webpack --watch",
    "test:api": "dredd --help",
